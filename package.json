    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.7",
    "tailwindcss": "2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "base64url": "^3.0.1",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
